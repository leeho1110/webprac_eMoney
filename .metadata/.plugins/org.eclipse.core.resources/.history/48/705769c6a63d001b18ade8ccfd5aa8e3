
// 페이지 로딩 시 로그인 포커스
	$(document).ready(function(){
		$("#idBox").focus();
	})
	
	function loginInputCheck() {
	    if ($("#idBox").val() == "") {
			alert("아이디 입력을 확인해주세요");
			$("#idBox").focus();
	    } else if ($("#pwBox").val() == "") {
			alert("비밀번호 입력을 확인해주세요");
			$("#pwBox").focus();
	    } else {
		$("#loginForm").submit();
	    }
	}