package com.test.webPrac.vo;

public class BoardVO {
	
	private String title;
	private String contentPath;
	private int writer;
	private String writedDate;
	
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return contentPath;
	}
	public void setContent(String content) {
		this.contentPath = content;
	}
	public int getWriter() {
		return writer;
	}
	public void setWriter(int writer) {
		this.writer = writer;
	}
	public String getWritedDate() {
		return writedDate;
	}
	public void setWritedDate(String writedDate) {
		this.writedDate = writedDate;
	}
	
	@Override
	public String toString() {
		return "BoardVO [title=" + title + ", content=" + contentPath + ", writer=" + writer + ", writedDate=" + writedDate
				+ "]";
	}
	
	
}
