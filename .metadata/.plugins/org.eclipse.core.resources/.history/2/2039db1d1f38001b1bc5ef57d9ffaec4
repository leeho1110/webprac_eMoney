// 각 항목별 통과 여부와 안내멘트 딕셔너리
 const loginCategory = {};

 loginCategory['idInput'] = [false, "아이디를 입력해주세요"];
 loginCategory['pwInput'] = [false,  "비밀번호를 입력해주세요"];


function loginInputCheck() {
    var tempFlag = true;
    for(var key in loginCategory){
        if(loginCategory[key][0] == false){
            alert(loginCategory[key][1]);
            tempFlag=false;
            break;
        }
    }

    if(tempFlag){
        $("#loginForm").submit();
    }
}